{
  "name": "thejerktracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 1456",
    "dev:turbo": "next dev --turbopack",
    "dev:dynamodb": "node scripts/start-dynamodb.js",
    "test:dynamodb": "node scripts/test-dynamodb.js",
    "test:dynamodb:full": "node scripts/test-dynamodb-full.js",
    "test:service": "node scripts/test-service.js",
    "build": "next build --turbopack",
    "build:vercel": "prisma generate && prisma db push && npm run db:seed && next build --turbopack",
    "build:vercel:test": "cross-env VERCEL=1 npm run build:vercel",
    "build:github": "npm run validate:build && npm run build:github:setup && cross-env GITHUB_ACTIONS=true next build",
    "build:github:setup": "node scripts/db-check.js && echo 'Database check complete'",
    "build:aws": "cp next.config.aws.js next.config.js && prisma generate && prisma db push && npm run db:seed:aws && next build",
    "build:aws:setup": "node scripts/aws-setup.js && echo 'AWS setup complete'",
    "validate:build": "node scripts/validate-build.js",
    "export": "next export",
    "build:static": "next build",
    "start": "next start",
    "postinstall": "prisma generate || echo 'Prisma generate failed during install'",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install || true",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:aws": "tsx prisma/seed.aws.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:check": "node scripts/db-check.js",
    "setup:db": "node scripts/setup-db.js",
    "setup:aws": "node scripts/aws-setup.js",
    "verify:vercel": "node scripts/verify-vercel.js",
    "verify:aws": "node scripts/verify-aws.js",
    "check:aws-status": "node scripts/check-aws-status.js",
    "check:deployment": "node scripts/check-deployment-status.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.894.0",
    "@aws-sdk/lib-dynamodb": "^3.894.0",
    "@emailjs/browser": "^4.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "dynamodb-local": "^0.0.35",
    "framer-motion": "^12.23.16",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@aws-amplify/cli": "^12.12.6",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
